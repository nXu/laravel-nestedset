{
  "name": "nxu/laravel-nestedset",
  "description": "Implementation of the Nested Set Model for Laravel Eloquent ORM.",
  "license": "MIT",
  "keywords": [
    "nxu",
    "laravel",
    "nested set",
    "eloquent",
    "baum"
  ],
  "authors": [
    {
      "name": "nxu",
      "email": "nxu@nxu.hu"
    }
  ],
  "require": {
    "php": ">=7.2",
    "laravel/framework": ">=6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4 | ^9.0",
    "orchestra/testbench": "4.* | 5.* | 6.*",
    "friendsofphp/php-cs-fixer": "^2.16",
    "doctrine/dbal": "^2.10"
  },
  "autoload": {
    "psr-4": {
      "Nxu\\NestedSet\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  }
}
